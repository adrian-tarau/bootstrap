<?xml version="1.0" encoding="ISO-8859-1"?>
<assets xmlns="https://net.microfalx/xsd/asset-1.0.xsd">

    <!-- Application  -->
    <asset-bundle id="app" path="app" name="Application" order="500">
        <depends-on-asset-bundle>bootstrap</depends-on-asset-bundle>
        <depends-on-asset-bundle>jquery-widget</depends-on-asset-bundle>
        <depends-on-asset-bundle>fa</depends-on-asset-bundle>
        <depends-on-asset-bundle>themify</depends-on-asset-bundle>

        <asset path="bootstrap.css" type="stylesheet"/>
        <asset path="adminator.css" type="stylesheet"/>
        <asset path="adminlte.css" type="stylesheet"/>
        <asset path="scroll_top.css" type="stylesheet"/>
        <asset path="component.css" type="stylesheet"/>
        <asset path="dropzone.css" type="stylesheet"/>
        <asset path="help.css" type="stylesheet"/>
        <asset path="chart.css" type="stylesheet"/>

        <asset path="utils.js" type="script"/>
        <asset path="logger.js" type="script"/>
        <asset path="core.js" type="script"/>
        <asset path="alert.js" type="script"/>
        <asset path="help.js" type="script"/>
        <asset path="ajax.js" type="script"/>
        <asset path="chart.js" type="script"/>
        <asset path="scroll_top.js" type="script"/>
        <asset path="search.js" type="script"/>
    </asset-bundle>

    <!-- Application  -->
    <asset-bundle id="help" path="help" name="Help" order="505">
        <depends-on-asset-bundle>app</depends-on-asset-bundle>

        <asset path="help.css" type="stylesheet"/>
        <asset path="admonition.css" type="stylesheet"/>

        <asset path="admonition.js" type="script"/>
    </asset-bundle>


    <!-- DataSet  -->
    <asset-bundle id="dataset" path="dataset" name="DataSet" order="505">
        <depends-on-asset-bundle>app</depends-on-asset-bundle>

        <asset path="sort.css" type="stylesheet"/>
        <asset path="grid.css" type="stylesheet"/>
        <asset path="formatting.css" type="stylesheet"/>
        <asset path="tools.css" type="stylesheet"/>

        <asset path="core.js" type="script"/>
        <asset path="sort.js" type="script"/>
    </asset-bundle>

    <!-- Custom Controllers  -->

    <asset-bundle id="controller" path="controller" name="Controllers" order="510">
        <depends-on-asset-bundle>dataset</depends-on-asset-bundle>

        <asset path="alert.js" type="script"/>
    </asset-bundle>

</assets>